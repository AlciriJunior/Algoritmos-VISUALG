Algoritmo "cd"

Var
//aumento de 15% no sal치rio
   s, ns, a : real
   n : caractere
Inicio
      escreva ("Digite o nome do funcionario: ")
      leia (n)
      escreva ("Digite o sal치rio atual: R$")
      leia (s)
      a <- (s*15)/100
      ns <- s + a
      escreval ("Funcion치rio ",n)
      escreval ("Novo sal치rio ",ns:5:2)
      

fimalgoritmo
